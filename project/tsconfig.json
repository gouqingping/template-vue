{
	"compilerOptions": {
		"composite": true,
		"jsx": "preserve",
		"paths": {
			"@/*": ["./src/*"],
			"_common/*": ["../common/*"],
			"@api/*": ["./src/api/*"],
			"@scss/*": ["./src/scss/*"],
			"@view/*": ["./src/view/*"],
			"@images/*": ["./src/images/*"],
			"@config/*": ["./src/config/*"],
			"@shared/*": ["./src/shared/*"],
			"@store/*": ["./src/config/store/*"],
			"@router/*": ["./src/config/router/*"],
			"@containers/*": ["./src/containers/*"],
			"@components/*": ["./src/components/*"]
		}
	},
	"include": ["./src", "../typings"]
}
