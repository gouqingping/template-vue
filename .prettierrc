{
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"endOfLine": "auto",
	"proseWrap": "always",
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": false
}
