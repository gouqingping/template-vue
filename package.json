{
  "name": "emodel",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host --mode NODE_ENV=dev",
    "pro": "vite --host --mode NODE_ENV=production",
    "build": "vue-tsc --noEmit && vite build --mode NODE_ENV=production",
    "build:production": "vite build --mode NODE_ENV=production",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prettier": "prettier -c --write **/*",
    "lint": "eslint src --fix --ext .js,.ts,.jsx,.tsx,.vue && npm run prettier --write --ignore-unknown",
    "prepare": "husky install",
    "pre-commit": "npx husky add .husky/pre-commit"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": [
      "eslint --fix ./packages --ext .vue,.ts",
      "npm run prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@elgis/request": "^1.0.9",
    "@elgis/map": "^1.0.1",
    "igu": "^1.1.7",
    "sass": "^1.41.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@types/jest": "^26.0.19",
    "@types/node": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-legacy": "^1.5.3",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.15",
    "ambiences": "^1.2.11",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^8.13.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.6.2",
    "sinon": "^11.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vite-plugin-mock": "^2.9.6",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.2.2"
  }
}
